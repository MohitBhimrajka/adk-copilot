# Copy this file to .env and fill in your values

# Use Vertex AI (1) or Google AI Studio (0)
GOOGLE_GENAI_USE_VERTEXAI=1

# Required for Vertex AI
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
GOOGLE_CLOUD_LOCATION=us-central1

# Required for RAG and Deployment Staging
GOOGLE_CLOUD_STORAGE_BUCKET=your-gcp-bucket-name
RAG_CORPUS_NAME="" # This will be populated by the setup script in a later phase

# Required for BigQuery integration
BQ_PROJECT_ID=your-gcp-project-id
BQ_DATASET_ID=support_tickets_dataset

# Optional: For CRM integration tool
CRM_API_KEY="your-crm-api-key-here"